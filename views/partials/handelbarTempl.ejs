<script id="entry-template" type="text/x-handlebars-template">





    {{#each this}}

    <div class="row results">
        
            <div class="col-xs-12 col-md-3">
                <a href="{{this.url}}" target="_blank"><img class="search-img" src="{{this.image_url}}"></a>
            </div>



            <div class="search-details col-xs-12 col-md-9">
               
                <h4><a href="{{this.url}}" target="_blank">{{this.name}}</a><span class="yelp-rating yelp-{{this.rating}}"></span></h4> 
                <p>{{this.address1}} {{this.address2}}<br />{{this.city}}, {{this.zip_code}}</p>
                <button class="search-result-goButton" data-id="{{this.id}}"><span id="numberAttendants">{{this.atendants}}</span> ppl going there</button>
            </div>
        

    </div>

    {{/each}}
    
    <div class="row">
        <div class="col-xs-12 col-md-6 col-md-offset-3 yelp-logo">
            <p>Search results provided by</p>
            <img src="yelp-2c.png">
            

        </div>
    </div>
    
